{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [2, { "extensions": [".js"] }],

        "import/prefer-default-export": [0],

        "arrow-parens": [2, "as-needed",  { "requireForBlockBody": false }]
    }
}